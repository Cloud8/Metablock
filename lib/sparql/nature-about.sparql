
PREFIX  dct:  <http://purl.org/dc/terms/>
PREFIX  npg:  <http://ns.nature.com/terms/>
PREFIX  skos: <http://www.w3.org/2004/02/skos/core#>
PREFIX  bibo: <http://purl.org/ontology/bibo/>
PREFIX  prism: <http://prismstandard.org/namespaces/basic/2.1/>
PREFIX  rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>

PREFIX  dcq:  <http://purl.org/dc/qualifier/1.0/>
PREFIX  dcam:   <http://purl.org/dc/dcam/>
PREFIX  dcmitype: <http://purl.org/dc/dcmitype/>
PREFIX  gnd:  <http://d-nb.info/gnd/>
PREFIX  foaf: <http://xmlns.com/foaf/0.1/>
PREFIX  fabio:<http://purl.org/spar/fabio/>
PREFIX  aiiso:<http://purl.org/vocab/aiiso/schema#>
PREFIX  urn:  <http://www.d-nb.de/standards/urn/>
PREFIX  shg:  <http://localhost/view/>
PREFIX  cito: <http://purl.org/spar/cito/>

## resource dump.
construct {
    <subject> ?p ?o .
    ?o ?x ?y . 
} where {
    <subject> ?p ?o .
    optional {
       ?o ?x ?y .
       ## EXISTS { ?o ?x ?y . }
       ## FILTER(!bound(?o))
       ## FILTER (isBlank(?o) && !isBlank(?y)) .
       FILTER (!isBlank(?y)) .
    }
    optional {
       <subject> dct:isPartOf ?o .
       ?o ?x ?y .
    }
    optional {
       <subject> dct:creator ?o .
       ?o ?x ?y .
    }
} 

